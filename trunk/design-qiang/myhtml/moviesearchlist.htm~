<script type="text/javascript">
    (function () {
        var input = $('#_j_dom_SearchInput');
        var btn = $('#j_dom_searchsubmit');
        new $.IKan.SearchTips(input, btn, { tipBoxWidth: '164px', strLength: 24 });
    })();

    (function() {
        var lis = $('div.pptv_topLogin .show li');
        var aels = lis[0].getElements('a');
        aels[1].addEvent('click', function(ev) { ev.stop(); $.IKan.User2.logout(); });
        $.IKan.User2.onLogined.subscribe(function(name, args) {
            aels[0].innerHTML = args[0].UserName;
            lis[0].css('display', '');
            lis[1].css('display', 'none');
            lis[2].css('display', 'none');
        });
        $.IKan.User2.onlogouted.subscribe(function(name, args) {
            lis[0].css('display', 'none');
            lis[1].css('display', '');
            lis[2].css('display', '');
        });
        $.IKan.User2.tryReadUserInfo();
    })();

    (function() {
        var btns = $('div.pptv_topLogin .show .more');
        var panels = $('div.pptv_topLogin .show .moreLi');
        for (var i = 0, len = btns.length; i < len; i++) { btns[i].attr('jsvalue', i); }
        var pt1, pt2, rt, el;
        $(document).addEvent('mousemove', function(ev) {
            pt1 = ev.page;
            for (var i = 0, len = panels.length; i < len; i++) {
                el = panels[i];
                if (el.css('display') != 'none') {
                    pt2 = el.position();
                    rt = { w: el.offsetWidth, h: el.offsetHeight + 24, x: pt2.x, y: pt2.y - 24 };
                    if (pt1.x < rt.x || pt1.x > rt.x + rt.w || pt1.y < rt.y || pt1.y > rt.y + rt.h) {
                        el.css('display', 'none');
                    }
                }
            }
        });
        btns.addEvent('mouseover', function() { var ix = parseInt(this.attr('jsvalue')); panels.css('display', 'none'); panels[ix].css('display', 'block'); });
    })();
</script>
